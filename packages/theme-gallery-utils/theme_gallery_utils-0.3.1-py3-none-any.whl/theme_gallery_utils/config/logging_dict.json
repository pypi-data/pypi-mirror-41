{
    "formatters": {
        "default": {
            "format": "%(asctime)s:%(levelname)s:%(name)s:%(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "default",
            "stream": "ext://sys.stdout"
        },
        "file": {
            "backupCount": 2,
            "class": "logging.handlers.RotatingFileHandler",
            "encoding": "utf-8",
            "formatter": "default",
            "maxBytes": 10485760
        }
    },
    "loggers": {
        "__main__": {
            "handlers": [
                "console",
                "file"
            ],
            "level": "DEBUG",
            "propagate": false
        },
        "theme_gallery_utils": {
            "handlers": [
                "console",
                "file"
            ],
            "level": "DEBUG",
            "propagate": false
        }
    },
    "root": {
        "handlers": [
            "console",
            "file"
        ],
        "level": "DEBUG"
    },
    "version": 1
}
