#!/Library/Frameworks/Python.framework/Versions/3.7/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Opacify==0.3.2','console_scripts','opacify'
__requires__ = 'Opacify==0.3.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Opacify==0.3.2', 'console_scripts', 'opacify')()
    )
