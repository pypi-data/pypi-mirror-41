{
  "name": "jupyterlab-zip",
  "version": "0.1.1",
  "description": "Utility to zip and download folders",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://git.corp.adobe.com/adobesensei/jupyterlab-zip",
  "license": "BSD-3-Clause",
  "author": "schatter",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://git.corp.adobe.com/adobesensei/jupyterlab-zip"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.18.6",
    "@jupyterlab/apputils": "^0.18.4",
    "@jupyterlab/coreutils": "^2.2.1",
    "@jupyterlab/docmanager": "^0.18.4",
    "@jupyterlab/services": "^3.2.1",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.6.0",
    "@jupyterlab/filebrowser": "0.18.6"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  },
  "directories": {
    "lib": "lib"
  }
}
